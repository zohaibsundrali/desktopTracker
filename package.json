{
  "name": "developer-tracker",
  "version": "1.0.0",
  "main": "electron/main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env IS_DEV=true electron .",
    "build": "vite build",
    "preview": "vite preview",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "@supabase/supabase-js": "^2.91.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "vite": "^7.3.1",
    "eslint": "^9.39.2",
    "@eslint/js": "^9.39.2",
    "globals": "^16.5.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "electron": "^40.0.0",
    "electron-builder": "^24.9.1",
    "concurrently": "^8.2.2",
    "wait-on": "^7.0.1",
    "cross-env": "^7.0.3"
  }
}